<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".screens.myCollection.MyCollectionFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            android:id="@+id/myProfileButton"
            layout="@layout/profile_button"
            tools:layout_editor_absoluteX="77dp"
            tools:layout_editor_absoluteY="32dp" />

        <ImageButton
            android:id="@+id/exitButton"
            android:layout_width="53dp"
            android:layout_height="58dp"
            android:layout_marginStart="8dp"
            android:backgroundTint="#00FFFFFF"
            android:src="@drawable/sharp_arrow_back_55"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/myProfileButton" />

        <include layout="@layout/add_button"
            android:id="@+id/addPlaceButton"
            />

        <include
            android:id="@+id/include2"
            layout="@layout/scrollable_table"
            tools:layout_editor_absoluteX="38dp"
            tools:layout_editor_absoluteY="130dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="המקומות שלי"
            android:textSize="24sp"
            app:layout_constraintBottom_toTopOf="@+id/include2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
